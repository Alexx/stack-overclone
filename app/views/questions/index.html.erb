<h1>Welcome to Stack Overflow</h1>

<% if @questions.any? %>
<ul>
  <% @questions.each do |question| %>
    <li><%= link_to question.title, question_path(question) %></li>
  <% end %>
</ul>
<% else %>
<p>there are no questions posted</p>
<% end %>




<p><%= link_to "Add a Question", new_question_path(@question) %></p>
